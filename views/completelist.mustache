<!doctype html>
<html lang="en" data-framework="javascript">
  <head>
    <meta charset="utf-8">
    <title>Simple Todo List</title>
    <link rel="stylesheet" href="css/styles.css">
  </head>
  <body>
    <section class="todoapp">
      <a href='/'>
        <div><p>Add more items</p></div>
      </a>
      <!-- <header class="header">
        <h1>todos</h1>
        <input class="new-todo" name="todoListItem"
        placeholder="What needs to be done?" autofocus>
      </header> -->
      <section class="main">
        <form action="/strikethrough"  method="post">
        {{#todoItems}}
          <ul class="todo-list">
            <li>{{.}}<input type="submit" name="complete"
              value = "Mark Complete" class="completedTasks"></li>
          </ul>
        {{/todoItems}}
        <del>{{.}}</del>
      </form>
      </section>
      <section class="showlist">

      </section>
    </section>
    {{name}}
    <script src="main.js"></script>
  </body>
</html>
